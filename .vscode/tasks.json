{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "编译队列插件",
			"type": "shell",
			"command": "mvn",
			"args": [
				"clean",
				"package"
			],
			"group": "build",
			"isBackground": false,
			"options": {
				"cwd": "${workspaceFolder}/queue-plugin"
			},
			"problemMatcher": [
				"$maven"
			],
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared"
			}
		}
	]
}